<div class="container" align="center">
    <p></p>
    <div class="media">
      <img class="media-object" src="data:image/png;base64," />
      
    <div class="media-body"></div>
    <p></p>

    <div class="authform">
	<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
	  <div class="form-group">
        <%= f.label " Usuario", class: 'fa fa-user' %><br />
	    <%= f.email_field :email, :autofocus => true, class: 'form-control', maxlength: "110", placeholder: "Nombre de usuario" %>
	  </div>

	  <div class="form-group">
	    <%= f.label :password, "  Contraseña", class: 'fa fa-lock'%><br />
	    <%= f.password_field :password, class: 'form-control', maxlength: "110", placeholder: "Contraseña" %>
	  </div>
	  <div class="actions">
	    <%= f.submit 'Ingresar', :class => 'btn btn-lg btn-success' %>
	  </div>
	  
	  <% if devise_mapping.rememberable? -%>
	    <div class="form-group">
	      <%= f.check_box :remember_me %>  Recordarme
	      <%= f.label :remember_me %>
	    </div>
	  <% end -%>
	   <%- if devise_mapping.recoverable? %>
            <%= link_to "Olvidé mi contraseña", new_password_path(resource_name), class: 'btn-litle' %>
          <% end -%>


	<% end %>

	</div>

</div>
